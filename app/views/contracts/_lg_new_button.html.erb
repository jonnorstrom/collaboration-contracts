<!-- new contract btn -->
<button type="button" class="btn btn-primary btn-lg col-xs-8 col-xs-offset-2" data-toggle="modal" data-target="#new_contract_modal">New Contract</button>
<!-- new contract modal -->
<div id="new_contract_modal" class="modal fade" role="dialog">
  <%= render 'contracts/new_modal' %>
</div>

<% if @error_messages %>
